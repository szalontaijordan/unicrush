<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600." xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unicrush.controller.EndGameSceneController">
   <children>
      <Label fx:id="gratText" layoutX="112.0" layoutY="65.0" prefHeight="17.0" prefWidth="378.0" />
      <Button fx:id="newGame" layoutX="176.0" layoutY="341.0" mnemonicParsing="false" onAction="#onNewGame" prefHeight="26.0" prefWidth="250.0" styleClass="new-game" text="New game" />
      <Label fx:id="currentScore" layoutX="112.0" layoutY="90.0" prefHeight="16.0" prefWidth="247.0" />
      <TableView fx:id="scoreTable" layoutX="111.0" layoutY="120.0" prefHeight="200.0" prefWidth="378.0">
        <columns>
          <TableColumn prefWidth="127.0" text="Player" />
          <TableColumn prefWidth="127.0" text="Level" />
            <TableColumn minWidth="0.0" prefWidth="122.0" text="Score" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
